<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <GeneDef ParentName="GeneJawBase">
    <defName>GEHD_Slime_Head</defName>
    <label>Slime head</label>
    <description>A head composed of gelatinous slime, its surface shifting with fluid, organic patterns. The translucent texture suggests a body more liquid than solid, alien yet alive.</description>
    <iconPath>UI/Icons/Genes/GEHD_Slime_Icon</iconPath>
    <forcedHeadTypes>
      <li>GEHD_Slime_Female</li>
      <li>GEHD_Slime_Male</li>
    </forcedHeadTypes>
    <displayOrderInCategory>97</displayOrderInCategory>
	<displayCategory>GEHD_Heads</displayCategory>
    <symbolPack>
      <prefixSymbols>
        <li><symbol>skull</symbol></li>
        <li><symbol>bone</symbol></li>
      </prefixSymbols>
    </symbolPack>
  </GeneDef>
</Defs>